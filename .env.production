# Production Environment Variables for LuxeBelle
# Copy this file to .env.production and update with your actual values

# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your_production_project_id"
VITE_SUPABASE_PUBLISHABLE_KEY="your_production_publishable_key"
VITE_SUPABASE_URL="https://your_production_project_id.supabase.co"

# Site Configuration
VITE_SITE_URL="https://your-domain.com"
VITE_SITE_NAME="LuxeBelle"
VITE_SITE_DESCRIPTION="Premium Online Boutique for Luxury Fashion"

# Analytics (Optional)
# VITE_GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"
# VITE_HOTJAR_ID="XXXXXX"

# Customer Support
# VITE_SUPPORT_EMAIL="support@luxebelle.com"
# VITE_SUPPORT_PHONE="+92-XXX-XXXXXXX"

# Social Media Links
# VITE_FACEBOOK_URL="https://facebook.com/luxebelle"
# VITE_INSTAGRAM_URL="https://instagram.com/luxebelle"
# VITE_TWITTER_URL="https://twitter.com/luxebelle"

# Feature Flags
VITE_ENABLE_GUEST_CHECKOUT="true"
VITE_ENABLE_RATE_LIMITING="true"
VITE_ENABLE_ANALYTICS="false"
